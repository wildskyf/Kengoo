{
  "applications": {
    "gecko": {
      "id": "kengoo@tw",
      "strict_min_version": "54.0"
    }
  },

  "name": "__MSG_extName__",
  "version": "2.0.1.4",
  "manifest_version": 2,
  "description": "__MSG_extDesc__",
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "parser.js",
      "kengoo.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true,
    "browser_style": true
  },
  "default_locale": "en",
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup.html",
	  "default_icon": "images/icon48.png"
  },
  "permissions": [
    "*://tw.stock.yahoo.com/*",
    "storage",
	  "notifications"
  ],
  "icons": {
    "128": "images/icon128.png",
	"48": "images/icon48.png",
    "16":"images/icon16.png"
  }

}
